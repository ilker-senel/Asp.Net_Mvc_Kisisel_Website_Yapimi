@model KisiselWebSitesi.Models.Siniflar.Ikonlar
@{
    ViewBag.Title = "YeniIkon";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2 class="mb-4">Yeni Ikon Ekle</h2>
<br />
<form class="form-group" method="post">
    @Html.LabelFor(x => x.Ikon)
    @Html.TextBoxFor(x => x.Ikon, new { @class = "form-control" })
    <br />
    @Html.LabelFor(x => x.Link)
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
    <br />
    <button class="btn btn-success">İkon Ekle</button>
</form>


